<p id="notice"><%= notice %></p>

<h1>Listing Websites</h1>

<table>
  <thead>
    <tr>
      <th>Address</th>
      <th>Phone</th>
      <th>Mobile</th>
      <th>Email</th>
      <th>Facebook</th>
      <th>Twitter</th>
      <th>Google</th>
      <th>Linkedin</th>
      <th>Youtube</th>
      <th>Dolar</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @websites.each do |website| %>
      <tr>
        <td><%= website.address %></td>
        <td><%= website.phone %></td>
        <td><%= website.mobile %></td>
        <td><%= website.email %></td>
        <td><%= website.facebook %></td>
        <td><%= website.twitter %></td>
        <td><%= website.google %></td>
        <td><%= website.linkedin %></td>
        <td><%= website.youtube %></td>
        <td><%= website.dolar %></td>
        <td><%= link_to 'Show', website %></td>
        <td><%= link_to 'Edit', edit_website_path(website) %></td>
        <td><%= link_to 'Destroy', website, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Website', new_website_path %>
